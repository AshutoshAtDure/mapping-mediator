{
  "identifiers[]": {
    "key": "identifier",
    "transform": {
      "function": "createBahmniFhirIdentifier"
    }
  },
  "person.names[]": {
    "key": "name",
    "transform": {
      "function": "createBahniFhirName"
    }
  },
  "person.gender": {
    "key": "gender",
    "transform": {
      "function": "getGenderFromCode"
    }
  },
  "person.dead": "deceasedBoolean",
  "person.deathDate": "deceasedDateTime",
  "person.addresses[]": {
    "key": "address",
    "transform": {
      "function": "getBahmniFhirAddress"
    }
  },
  "person.uuid": "id"
}
